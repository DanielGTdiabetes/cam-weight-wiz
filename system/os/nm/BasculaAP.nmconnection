[connection]
id=BasculaAP
uuid=${GENERATE_UUID}
type=wifi
autoconnect=false
autoconnect-priority=-999
interface-name=wlan0
permissions=

[wifi]
mode=ap
ssid=Bascula-AP
band=bg

[wifi-security]
key-mgmt=wpa-psk
psk=Bascula1234

[ipv4]
address1=192.168.4.1/24
gateway=192.168.4.1
method=shared

[ipv6]
method=ignore

